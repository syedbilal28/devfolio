<svg width="180" height="180" viewBox="0 0 180 180" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle -->
  <circle cx="90" cy="90" r="85" fill="#0C121D" stroke="#fec544" stroke-width="2.5"/>
  
  <!-- Network nodes -->
  <!-- Center node -->
  <circle cx="90" cy="90" r="16" fill="#fec544"/>
  <circle cx="90" cy="90" r="11" fill="#ffd166" opacity="0.6"/>
  
  <!-- Top nodes -->
  <circle cx="90" cy="45" r="14" fill="#7a99d9"/>
  <circle cx="56" cy="56" r="11" fill="#fec544" opacity="0.9"/>
  <circle cx="124" cy="56" r="11" fill="#7a99d9" opacity="0.9"/>
  
  <!-- Bottom nodes -->
  <circle cx="90" cy="135" r="14" fill="#7a99d9"/>
  <circle cx="56" cy="124" r="11" fill="#fec544" opacity="0.9"/>
  <circle cx="124" cy="124" r="11" fill="#7a99d9" opacity="0.9"/>
  
  <!-- Side nodes -->
  <circle cx="45" cy="90" r="11" fill="#fec544" opacity="0.8"/>
  <circle cx="135" cy="90" r="11" fill="#7a99d9" opacity="0.8"/>
  
  <!-- Connection lines -->
  <!-- From center to top -->
  <line x1="90" y1="74" x2="90" y2="59" stroke="#fec544" stroke-width="1.8" opacity="0.7"/>
  <line x1="76" y1="76" x2="65" y2="65" stroke="#fec544" stroke-width="1.3" opacity="0.6"/>
  <line x1="104" y1="76" x2="115" y2="65" stroke="#7a99d9" stroke-width="1.3" opacity="0.6"/>
  
  <!-- From center to bottom -->
  <line x1="90" y1="106" x2="90" y2="121" stroke="#7a99d9" stroke-width="1.8" opacity="0.7"/>
  <line x1="76" y1="104" x2="65" y2="115" stroke="#fec544" stroke-width="1.3" opacity="0.6"/>
  <line x1="104" y1="104" x2="115" y2="115" stroke="#7a99d9" stroke-width="1.3" opacity="0.6"/>
  
  <!-- From center to sides -->
  <line x1="74" y1="90" x2="56" y2="90" stroke="#fec544" stroke-width="1.3" opacity="0.6"/>
  <line x1="106" y1="90" x2="124" y2="90" stroke="#7a99d9" stroke-width="1.3" opacity="0.6"/>
  
  <!-- Inter-node connections -->
  <line x1="56" y1="56" x2="45" y2="90" stroke="#fec544" stroke-width="1.1" opacity="0.5"/>
  <line x1="124" y1="56" x2="135" y2="90" stroke="#7a99d9" stroke-width="1.1" opacity="0.5"/>
  <line x1="56" y1="124" x2="45" y2="90" stroke="#fec544" stroke-width="1.1" opacity="0.5"/>
  <line x1="124" y1="124" x2="135" y2="90" stroke="#7a99d9" stroke-width="1.1" opacity="0.5"/>
  
  <!-- Additional connections -->
  <line x1="56" y1="56" x2="90" y2="45" stroke="#fec544" stroke-width="0.9" opacity="0.4"/>
  <line x1="124" y1="56" x2="90" y2="45" stroke="#7a99d9" stroke-width="0.9" opacity="0.4"/>
  <line x1="56" y1="124" x2="90" y2="135" stroke="#fec544" stroke-width="0.9" opacity="0.4"/>
  <line x1="124" y1="124" x2="90" y2="135" stroke="#7a99d9" stroke-width="0.9" opacity="0.4"/>
</svg>
